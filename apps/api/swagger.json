      "UnauthorizedErrorDto": {
        "type": "object",
        "properties": {
          "statusCode": { "type": "number" },
          "message": { "type": "string" }
        },
        "required": ["statusCode", "message"]
      },
      "ConflictErrorDto": {
        "type": "object",
